    "bestgain = 0\n",
    "for x in range(401):\n",
    "  for y in range(201):\n",
    "    if (x/60 + y/50 <= 8):\n",
    "      gain = 20*x + 30*y\n",
    "      if (gain > bestgain):\n",
    "        bestgain = gain\n",
    "        bestX = x\n",
    "        bestY = y\n",
    "print(\"x=\" + str(bestX) + \", y=\" + str(bestY) + \" -> \" + str(bestgain))"
